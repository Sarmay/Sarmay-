<!--miniprogram/pages/custom-tab-bar/index.wxml-->
<van-tabbar
  active="{{ active }}"
  active-color="#07c160"
  inactive-color="#000"
  bind:change="onChange"
  fixed
  style="font-family: kaz;"
>
  <van-tabbar-item name="personal" icon="manager-o">مەنىڭ</van-tabbar-item>
  <van-tabbar-item name="questions" icon="smile-comment-o">كەڭەس سۇراۋ</van-tabbar-item>
  <van-tabbar-item name="store" icon="shop-o">دۇكەن</van-tabbar-item>
  <van-tabbar-item name="index" icon="home-o">باس بەت</van-tabbar-item>
</van-tabbar>